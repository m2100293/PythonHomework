   "source": [
    "convert = lambda a: 'КЛЮЧ <<'+str(a[0])+'>> --> ЗНАЧЕНИЕ '+str(a[1])\n",
    "\n",
    "def DisplayAllElements(data):\n",
    "    print(*map(convert,data.items()),sep='\\n')\n",
    "\n",
    "\n",
    "\n",
    "my_dict={\"key1\": 2, \"key3\": False, \"Приветствие\": \"Hello\"}\n",
    "DisplayAllElements(my_dict)"
   ]
